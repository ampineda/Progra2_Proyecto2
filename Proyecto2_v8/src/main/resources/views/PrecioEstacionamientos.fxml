<Pane prefHeight="400.0" prefWidth="590.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PrecioEstacionamientosController">
   <children>
      <ImageView fitHeight="337.0" fitWidth="527.0" layoutX="58.0" layoutY="104.0" pickOnBounds="true" preserveRatio="true">
