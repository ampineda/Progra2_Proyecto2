      <TextField fx:id="txtPrecio" layoutX="254.0" layoutY="132.0" />
      <Label layoutX="86.0" layoutY="60.0" prefHeight="17.0" prefWidth="157.0" text="Tipo de Vehiculo:" />
      <Label layoutX="88.0" layoutY="94.0" prefHeight="17.0" prefWidth="157.0" text="Cantidad de Segundos:" />
      <Label layoutX="87.0" layoutY="136.0" prefHeight="17.0" prefWidth="157.0" text="Precio por tiempo:" />
      <Button fx:id="bntGrabar" layoutX="157.0" layoutY="247.0" mnemonicParsing="false" onAction="#grabaDatos" prefHeight="25.0" prefWidth="91.0" text="Grabar" />
